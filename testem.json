{
  "framework": "mocha",
  "src_files": [
    "node_modules/chai/chai.js",
    "src/*",
    "tests/*.js"
  ],
  "launchers": {
    "node": {
      "command": "./node_modules/.bin/mocha --require src/index.js --require node_modules/chai/chai.js -R tap tests/*.js",
      "protocol": "tap"
    }
  },
  "launch_in_dev": [
    "node"
  ]
}
